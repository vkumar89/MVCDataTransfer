
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index1</title>
</head>
<body>
    <div>

        <table border="1" align="center" width="15%">
            <caption>Product Details</caption>
            <tr>
                <td>Id</td>
                <td>@TempData.Peek("Id")</td>
            </tr>
            <tr>
                <td>Name</td>
                <td>@TempData.Peek("Name")</td>
            </tr>
            <tr>
                <td>Price</td>
                <td>
                    @{
                        double? price = Convert.ToDouble(TempData.Peek("price"));
                        price = price + (price * 10 / 100);
                        <text>@price
                        </text>
                    }
                </td>
            </tr>

        </table>
        <center>Click to launch new <a href="/Test/Display1">Page</a>.</center>



    </div>
</body>
</html>
